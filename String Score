class Solution {
    public int scoreOfString(String s) {
        int sum = 0;
        for(int i = 0 ; i < s.length() - 1; i++){
            char ch = s.charAt(i);
            char ch1 = s.charAt(i+1);
            int count = ch;
            int count1 = ch1;

            sum = Math.abs(count-count1) +sum;

            

        }
        return sum;
        
        

        
    }
}
